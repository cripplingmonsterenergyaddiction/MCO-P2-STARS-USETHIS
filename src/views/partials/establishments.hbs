<section>
    <h1 id="establishment-title">Establishment</h1>
    <hr />
</section>
<div class="first-line">
    {{#each restaurant_row1}}
    <div class="restos">
        <a href="/resto_review/estb-review.html">
            <img src="{{restoPic}}" alt="establishment" id="resto-pic" />
            <section class="title-bar-box">
                <h2 id="resto-name">{{restoName}}</h2>
                <section class="ratings">
                    <h4 id="main-rating">{{main_rating}}</h4>
                    <h5 id="out-of">{{outOF}}</h5>
                    <h4 id="overall-rating">{{overall}}</h4>
                    <img src="{{star_img}}" alt="star" id="rate" />
                </section>
            </section>
            <p class="description">{{description}}</p>
        {{!-- <div class="rating-count"> --}}
            {{!-- <span class="helpful">Votes</span>
            <i class="fas fa-thumbs-up thumbs-up-icon"></i>
            <span class="num-votes likes">{{noOfLike}}</span>
            <i class="fas fa-thumbs-down thumbs-down-icon"></i>
            <span class="num-votes dislikes">{{noOfDislike}}</span> --}}
        {{!-- </div> --}}
    </div>     
    {{/each}}
</div>

